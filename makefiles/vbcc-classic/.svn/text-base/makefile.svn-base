# makefile for GNU make (automatically generated by makemake)
# NOTE: indent with TAB in GNU makefiles!

# paths are relative to the project directory (current directory during make)

EXE    = bin/vbcc-classic/iGameLite
OBJDIR = o/vbcc-classic/

# target 'all' (default target)

all : $(EXE)

$(OBJDIR)igamelite.o : igamelite.c
	vbcc:bin/vc $(shell vbccprefs) -c -o $(OBJDIR)igamelite.o igamelite.c

OBJS = $(OBJDIR)igamelite.o

$(EXE) : $(OBJS)
	vbcc:bin/vc $(shell vbccprefs) -o $(EXE) $(OBJS)

# target 'clean'

clean:
	-delete $(EXE)
	-delete $(OBJDIR)\*.o
